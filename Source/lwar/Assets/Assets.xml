<?xml version="1.0" encoding="utf-8"?>

<AssetsProject>
	<Configuration>
		<Identifier Name="lwar" />
		<TempDirectory Name="../obj/Assets" />

		<OutputDirectory Name="../../Binaries x64/Debug Direct3D11" Platform="x64" Debug="true" Graphics="Direct3D11" />
		<OutputDirectory Name="../../Binaries x64/Debug OpenGL3" Platform="x64" Debug="true" Graphics="OpenGL3" />
		<OutputDirectory Name="../../Binaries x64/Release Direct3D11" Platform="x64" Debug="false" Graphics="Direct3D11" />
